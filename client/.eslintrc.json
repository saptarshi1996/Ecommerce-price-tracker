{
  "extends": "next/core-web-vitals",
  "rules": {
    "semi": [
      2,
      "never"
    ],
    "no-trailing-spaces": 2,
    "indent": [
      "error",
      2
    ],
    "eol-last": 2,
    "quotes": [
      2,
      "single"
    ],
    "@typescript-eslint/no-explicit-any": "off",
    "camelcase": "error",
    "no-var": "warn",
    "no-unused-vars": "error"
  }
}